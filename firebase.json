{
  "hosting": {
    "site": "bidtech",
    "public": "public",
    "rewrites": [
      {
        "source": "**",
        "appHosting": {
          "backendId": "bidtech"
        }
      }
    ]
  },
  "apphosting": {
    "backendId": "bidtech",
    "region": "us-east5",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log"
    ]
  },
  "emulators": {
    "apphosting": {
      "port": 5002,
      "rootDirectory": "./",
      "startCommand": "npm run dev"
    },
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "extensions": {}
}
